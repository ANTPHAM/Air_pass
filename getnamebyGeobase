>>> from GeoBases import GeoBase
>>> geo_a = GeoBase(data='ori_por', verbose=False)
>>> [geo_a.get(k,'name') for k in ('LHR','MCO','JFK','LAX','BKK','LAS','SFO','ORD','CDG','DXB')]
['London Heathrow Airport', 'Orlando International Airport', 'John F Kennedy International Airport', 'Los Angeles International Airport', 'Suvarnabhumi', 'Mc Carran International Airport', 'San Francisco International Airport', "Chicago O'Hare International Airport", 'Paris - Charles-de-Gaulle', 'Dubai International Airport']
>>>
